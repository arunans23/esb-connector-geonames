<component name="geonames-postalCodes" type="synapse/template">
    <subComponents>
        <component name="postalCodeSearch">
            <file>postalCodeSearch.xml</file>
            <description>Returns a list of postal codes and places for the place name/postal code query.</description>
        </component>
        <component name="listPlacesForPostalCode">
            <file>listPlacesForPostalCode.xml</file>
            <description>Returns a list of places for the given postal code.</description>
        </component>
        <component name="listNearbyPostalCodes">
            <file>listNearbyPostalCodes.xml</file>
            <description>Returns a list of postal codes and places for the lat/lng query.</description>
        </component>
    </subComponents>
</component>